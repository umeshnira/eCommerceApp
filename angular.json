{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Ecart": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Ecart",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": [
              "src/assets/products/js/jquery-2.1.4.min.js",
              "src/assets/products/js/jquery-ui.js",
              "src/assets/products/js/easyResponsiveTabs.js",
              "src/assets/products/js/creditly.js",
              "src/assets/products/js/SmoothScroll.min.js",
              "src/assets/products/js/move-top.js",
              "src/assets/products/js/easing.js",
              "src/assets/products/js/imagezoom.js",
              "src/assets/products/js/jquery.flexisel.js",
              "src/assets/products/js/jquery.flexslider.js",
              "src/assets/products/js/bootstrap.js",
              "src/assets/products/js/carousel.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Ecart:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Ecart:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Ecart:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": [
              "src/assets/products/js/jquery-2.1.4.min.js",
              "src/assets/products/js/jquery-ui.js",
              "src/assets/products/js/easyResponsiveTabs.js",
              "src/assets/products/js/creditly.js",
              "src/assets/products/js/SmoothScroll.min.js",
              "src/assets/products/js/move-top.js",
              "src/assets/products/js/easing.js",
              "src/assets/products/js/imagezoom.js",
              "src/assets/products/js/jquery.flexisel.js",
              "src/assets/products/js/jquery.flexslider.js",
              "src/assets/products/js/bootstrap.js",
              "src/assets/products/js/carousel.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Ecart:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Ecart:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "Ecart"
}
